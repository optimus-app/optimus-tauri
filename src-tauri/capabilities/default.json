{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "websocket:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://localhost:8080/*" }]
    },
    "log:default"
  ]
}